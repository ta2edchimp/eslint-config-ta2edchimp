{
  "extends": "./.eslintrc.json",
  "env": {
    "es6": true
  },
  "rules": {
    "no-var": [ 2 ],
    "object-shorthand": [ 2, "always" ],
    "prefer-arrow-callback": [ 2 ],
    "prefer-const": [ 2 ],
    "prefer-reflect": [ 1 ],
    "prefer-rest-params": [ 1 ],
    "prefer-spread": [ 1 ],
    "prefer-template": [ 1 ],
    "require-yield": [ 2 ],
    "sort-imports": [ 1 ],
    "template-curly-spacing": [ 2, "always" ],
    "yield-star-spacing": [ 2, "before" ]
  }
}
